programa
{
	funcao inicio()
	{
		inteiro n
		escreva ("Insira um número maior que zero: ")
		leia (n)
		se (n > 10)
		{
			escreva ("\nO número é ", n, ".\n")
		}
		senao
		{
			escreva ("\nO número é menor que dez.\n")
		}
	}
}
