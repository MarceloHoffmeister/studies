programa
{
	funcao inicio()
	{
		real ang1, ang2
		escreva ("Insira o primeiro ângulo do triângulo: ")
		leia (ang1)
		escreva ("\nInsira o segundo ângulo do triângulo: ")
		leia (ang2)
		escreva ("\nO terceiro ângulo do triângulo é ", 180.0 - (ang1 + ang2), "°.\n")
	}
}
