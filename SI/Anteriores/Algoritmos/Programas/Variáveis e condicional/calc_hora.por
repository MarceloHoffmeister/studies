programa
{
	funcao inicio()
	{
		inteiro h1, h2, min, hora_dia

		escreva("Que horas são: ")
		leia (h1, h2)

		min = (60 * h1) + h2

		escreva ("Passram desse dia ", min, " minutos")
		
		
	}
}
