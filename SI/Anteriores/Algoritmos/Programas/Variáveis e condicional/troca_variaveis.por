programa
{
	funcao inicio()
	{
		inteiro a, b, troca
		escreva ("Digite um valor inteiro: ")
		leia (a)
		escreva ("Digite outro valor inteiro: ")
		leia (b)
		troca = a
		a = b
		b = troca
		escreva ("Os valores foram trocados!\n\n")
		escreva ("O valor de A é: ", a, " e o de B é: ", b)  
	}
}
