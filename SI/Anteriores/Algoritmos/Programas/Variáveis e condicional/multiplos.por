programa
{
	funcao inicio()
	{
		inteiro n, v
		escreva ("Insira um número: ")
		leia (n)
		v = n % 3
		se (v == 0)
		{
			escreva ("\nÉ múltiplo de 3.\n")
		}
		senao
		{
			escreva ("\nNão é múltiplo de 3.\n")
		}
	}
}
