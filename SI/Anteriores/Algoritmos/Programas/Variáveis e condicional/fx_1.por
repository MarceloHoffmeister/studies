programa
{
	funcao inicio()
	{
		real x, Fx
		escreva ("Digite o valor de x: ")
		leia (x)

		Fx = 8 / (2 - x)

		escreva ("\nO valor de x aplicado à função F(x)=8/(2-x) é igual a ", Fx, ".\n")
	}
}
